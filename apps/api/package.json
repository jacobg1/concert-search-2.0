{
  "name": "concert-search-api",
  "version": "1.0.0",
  "description": "concert-search-back-end",
  "author": "jacob-greenwald",
  "private": true,
  "scripts": {
    "build": "rm -rf ./dist && nest build --webpack",
    "build:watch": "npm run build -- --watch",
    "deploy": "serverless deploy --stage prod",
    "deploy:test": "serverless deploy --stage test",
    "dev": "WITH_MOCKS=true npm run build:watch & npm run start",
    "start": "serverless offline start --reloadHandler",
    "test": "jest",
    "test:watch": "jest --watch --collectCoverage=false",
    "lint": "eslint .",
    "check-types": "tsc --noEmit"
  },
  "dependencies": {
    "@nestjs/common": "^11.1.3",
    "@nestjs/core": "^11.1.3",
    "archive-search": "^1.1.15",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "lodash.chunk": "4.2.0",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1"
  },
  "devDependencies": {
    "@darraghor/eslint-plugin-nestjs-typed": "^6.9.1",
    "@nestjs/testing": "^11.1.3",
    "@repo/eslint-config": "*",
    "@repo/jest-config": "*",
    "@repo/mock-data": "*",
    "@repo/ts-config": "*",
    "@types/aws-lambda": "8.10.150",
    "@types/aws-serverless-express": "^3.3.10",
    "@types/express": "5.0.3",
    "@types/jest": "30.0.0",
    "@types/lodash.chunk": "4.2.9",
    "@types/node": "^22.15.34",
    "@types/supertest": "^6.0.3",
    "@typescript-eslint/eslint-plugin": "8.35.1",
    "@typescript-eslint/parser": "8.35.1",
    "dotenv-webpack": "^8.1.1",
    "esbuild-register": "^3.6.0",
    "jest": "^30.0.3",
    "msw": "^2.10.3",
    "nock": "^13.5.6",
    "serverless-offline": "^13.9.0",
    "terser-webpack-plugin": "^5.3.14",
    "ts-jest": "^29.4.0",
    "ts-loader": "^9.5.4",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3"
  }
}
